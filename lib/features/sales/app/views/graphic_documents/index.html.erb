<%= order_header %>

<% add_contextual_menu_item :possible_actions do %>
  <%= new_graphic_item_link(GraphicDocument) %>
<% end %>

<% display_user_spool_in_contextual_menu %>

<h1>Tous les documents graphiques du dossier</h1>
<div class="resources">
  <% unless @enabled_graphic_documents.empty? %>
    <%= render :partial => 'graphic_items/graphic_item_summary', :collection => @enabled_graphic_documents %>
  <% else %>
    <p>Il n'y a aucun document graphique dans ce dossier<p>
  <% end %>
</div>
