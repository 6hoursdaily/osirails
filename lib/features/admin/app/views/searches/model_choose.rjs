unless params[:model]==""
  page.remove("criteria_list")
  page.insert_html :bottom , "criteria",{ :partial => "model_choose"  ,:locals =>{:model => params[:model], :line=> params[:line]}}
  page.visual_effect :appear , "search_type"
  page.visual_effect :appear , "submit_button"
else
  page.alert("Vous devez choisir un model dans lequel effectuer une recherche")
end
