country = Country.find_by_name(params[:country_name])
unless country.nil?
  city =City.find_by_name_and_country_id(params[:city_name], country.id)

  if !(city.nil?)
    page["#{params[:address_name]}_address_city_zip_code"].value = city.zip_code

    page["#{params[:address_name]}_address_country_name"].value = country.name
  end
end